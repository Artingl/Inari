{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}"
            ],
            "defines": [
                "CONFIG_ARCH_I686=1"
            ],
            "cStandard": "c99",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-std=c99 -O2 --include \"config.h\" -I . -fno-stack-protector -nostdlib -ffreestanding -c -m32 -mfpmath=387"
            ]
        }
    ],
    "version": 4
}